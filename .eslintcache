 Navbar = ({ fixed, userLoggedIn }) => {\n\n  const cartIcon = <FontAwesomeIcon icon={faShoppingCart} size=\"2x\"/>\n  const [ activeItem, setActiveItem ] = useState('home')\n\n  const handleItemClick = (e, { name }) => {\n    setActiveItem(name)\n  }\n\n  return (\n\n    <Menu\n      fixed={fixed ? 'top' : null}\n      inverted={!userLoggedIn}\n      pointing={!fixed}\n      secondary={!fixed}\n      className={`${userLoggedIn ? \"inactiveUser\" : \"activeUser\" } navbar`}\n      size='large'\n    >\n      <Container>\n        <Menu.Item \n          as={Link} \n          to=\"/\"\n          name=\"home\"\n          active={activeItem === 'home'}\n          onClick={handleItemClick}>Home</Menu.Item>\n\n        {\n          userLoggedIn && \n          <>\n            <Menu.Item \n              as={Link}\n              to=\"/products\"\n              name=\"products\"\n              active={activeItem === 'products'}\n              onClick={handleItemClick}>Products</Menu.Item> {/* The MAY or MAY NOT have access to all products */}\n\n            <Menu.Item \n              as={Link}\n              to=\"/profile\" \n              name=\"profile\"\n              active={activeItem === 'profile'}\n              onClick={handleItemClick}>Profile</Menu.Item>\n\n            <Menu.Item \n              as={Link}\n              to=\"/inventory\"\n              name=\"inventory\"\n              active={activeItem === 'inventory'}\n              onClick={handleItemClick}>Inventory</Menu.Item> {/* The user has an inventory they're selling */}\n          </>\n        }\n        <Menu.Item position='right'>\n          {\n            !userLoggedIn ?\n            <Button as={Link} to=\"/login\" inverted={!userLoggedIn} primary={userLoggedIn}>\n              Log in\n            </Button> :\n            <Button as={Link} to=\"/cart\" animated='vertical' inverted secondary={true}>\n              <Button.Content hidden>10 items</Button.Content>\n              <Button.Content visible>\n                <Icon name='shop' />\n              </Button.Content>\n            </Button>\n          }\n            <Button \n              as={Link} \n              to={`${userLoggedIn ? \"/\" : \"/signup\"}`} \n              color={`${userLoggedIn && \"purple\"}`} \n              inverted={true} \n              secondary={userLoggedIn}\n              className=\"navbar__multifunctionBtn\"> \n                {userLoggedIn ? \"Log out\" : \"Sign Up\"}\n              </Button>\n        </Menu.Item>\n      </Container>\n    </Menu>\n  );\n}\nexport default Navbar;","/Users/marlonbraga/Desktop/shopify/shopify-backend-assessment/shopify-frontend/src/components/homepageHeading/HomepageHeading.js",["73","74"],"import React from 'react';\nimport { Link } from 'react-router-dom';\nimport { Button, Header, Icon, Container } from 'semantic-ui-react';\n\nimport { FontAwesomeIcon } from '@fortawesome/react-fontawesome'\nimport { faStoreAlt, faDoorOpen } from '@fortawesome/free-solid-svg-icons'\nimport './Styles.scss';\n\nconst HomepageHeading = ({ headers, userLoggedIn }) => {\n\n  const iconStore = <FontAwesomeIcon icon={faStoreAlt} size=\"x2\" />\n  const iconDoor = <FontAwesomeIcon icon={faDoorOpen} size=\"x2\" />\n\n  return(\n    <Container text className=\"homepageHeading\">\n      <Header\n        className=\"homepageHeading__mainHeader\"\n        as='h1'\n        content={headers.header}\n        inverted={!userLoggedIn}\n      />\n      <Header\n        className=\"homepageHeading__callAction\"\n        as='h2'\n        content={headers.callAction}\n        inverted={!userLoggedIn}\n      />\n      {\n      !userLoggedIn &&\n        <div>\n          <Button as={Link} to=\"/signup\" primary size='huge'>\n            {headers.buttonOne} {iconStore}\n          </Button>\n          <Button as={Link} to=\"/signup\" positive size='huge'>\n            {headers.buttonTwo} {iconDoor}\n          </Button>\n        </div>\n      }\n    </Container>\n  )\n}\n\nexport default Home